    <!-- Products Start -->
    <div class="container-fluid pt-5 pb-3">
        <h2 class="section-title position-relative text-uppercase mx-xl-5 mb-4"><span class="bg-secondary pr-3">ĐIỆN THOẠI</span></h2>
        <div class="row px-xl-5">
            {{#each dienthoais}}
            <div class="col-lg-3 col-md-4 col-sm-6 pb-1">
                <div class="product-item bg-light mb-4">
                    <div class="product-img position-relative overflow-hidden">
                        <img class="img-fluid w-100" src="{{this.img}}" alt="">
                        <div class="product-action">
                            
                            <a class="btn btn-outline-dark btn-square" href=""><i class="far fa-heart"></i></a>
                            <a class="btn btn-outline-dark btn-square" href=""><i class="fa fa-sync-alt"></i></a>
                            {{!-- <a class="btn btn-outline-dark btn-square" href="/products/{{this._id}}"><i class="fa fa-search"></i></a> --}}
                        </div>
                    </div>
                    <div class="text-center py-4">
                        <a class="h6 text-decoration-none text-truncate" href="/products/{{this._id}}">{{this.name}}</a>
                        <div class="d-flex align-items-center justify-content-center mt-2">
                            <h5>{{this.gia}}</h5><h6 class="text-muted ml-2"><del>{{this.gia2}}</del></h6>
                        </div>
                        <div class="d-flex align-items-center justify-content-center mb-1">
                            <small class="fa fa-star text-primary mr-1"></small>
                            <small class="fa fa-star text-primary mr-1"></small>
                            <small class="fa fa-star text-primary mr-1"></small>
                            <small class="fa fa-star text-primary mr-1"></small>
                            <small class="fa fa-star text-primary mr-1"></small>
                            <small>(99)</small>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
            <div class="col-12">
                        <nav>
                            <ul class="pagination justify-content-center">
                                <li class="page-item {{#unless dienthoais.hasPrevPage}}disabled{{/unless}}">
                                    <a class="page-link" href="?page={{dienthoais.prevPage}}">Previous</a>
                                </li>
                                {{#each pages}}
                                    <li class="page-item {{#if ../dienthoais.pageCond}} active {{/if}}">
                                        <a class="page-link" href="?page={{this}}">{{this}}</a>
                                    </li>
                                {{/each}}
                                <li class="page-item {{#unless dienthoais.hasNextPage}}disabled{{/unless}}">
                                    <a class="page-link" href="?page={{dienthoais.nextPage}}">Next</a>
                                </li>
                            </ul>
                        </nav>
            </div>
                
            </div>
        </div>
    </div>
    <!-- Products End -->


